<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <link href="css/fileuploader.css" rel="stylesheet" type="text/css">	
    <link href="css/bootstrap.css" rel="stylesheet" type="text/css">
    <link href="css/ub.css" rel="stylesheet" type="text/css">	
</head>
<body>
	<div id="ubaPlayer"></div>
	<div class="container">
		<div class="hero-unit">
			<h1>Music Share</h1>
		</div>
		<div class="row">
			<div class="span8 offset2">
				<ul class="controls well">
				  <% files.forEach(function(file){ %>
				  	<% var decoded = decodeURIComponent(file); %>
				  	<% file = encodeURIComponent(file); %>
				    <li><a class="audioButton" href="/uploads/<%= file %>"><%= decoded %></a></li>
				  <% }) %>
				</ul>
			</div>
		</div>
		<div class="row">
			<div class="span8 offset2">
				<div id="bootstrapped-fine-uploader"></div>
			</div>
		</div>
	</div>
 	<script src="js/jquery.js" type="text/javascript"></script>
    <script src="js/jquery.ubaplayer.js" type="text/javascript"></script>
    <script src="js/fileuploader.js" type="text/javascript"></script>
	<script src="js/app.js" type="text/javascript"></script>
</body>
</html>